    13                                  ; our data is declared here (the variables needed by our program)
    14                                  segment data use32 class=data
    15                                      ; ...
    16 00000000 FFFFFFFF                a dd -1
    17 00000004 02                      b db 2
    18 00000005 5F                      c db 95
    19 00000006 01000000                e dd 1
    20 0000000A 0100000000000000        x dq 1 
    21 00000012 0000000000000000        rez dq 0
    22                                  ; our code starts here
    23                                  segment code use32 class=code
    24                                      start:
    25                                          ; ...
    26 00000000 B064                        mov AL,100
    27 00000002 2A05[05000000]              sub AL,[c]
